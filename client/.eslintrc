{
    "extends": "airbnb",
    "plugins": [
        "react"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "rules":{
      "linebreak-style": 0,
      "no-shadow": "off"
    },
    "env":{
      "browser": true
    }
}
